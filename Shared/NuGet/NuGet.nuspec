  <files>
    <file src="..\..\lib\Zebble.Google.iOS.*" target="lib\Xamarin.iOS10\" />
    <file src="..\..\lib\Zebble.Google.Android.*" target="lib\MonoAndroid10\" />
    <file src="..\..\lib\Zebble.Google.UWP.*" target="lib\UAP10\" />
